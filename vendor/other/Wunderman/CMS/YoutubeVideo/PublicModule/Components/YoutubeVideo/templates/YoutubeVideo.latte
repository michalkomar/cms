{*
 * @var $article
 *}
{block content}
<script type="text/javascript">
	$( window ).load(function(){
		var YoutubeVideoID = {$params['youtubeHash']};
		YoutubeVideoContainer = '#Youtube-video-container-'+YoutubeVideoID;
		var YoutubeVideoIframe = '#Youtube-video-iframe-'+YoutubeVideoID;
		var YoutubeVideoPlay = '#Youtube-video-play-'+YoutubeVideoID;
		var videoOverlayId = '#Youtube-video-overlay-'+YoutubeVideoID;
		var videoOverlayClose = '#Youtube-video-overlay-close-'+YoutubeVideoID;

		function showVideo(id) {
			$(YoutubeVideoIframe).attr('src','//www.youtube.com/embed/'+ id + '?rel=0&autoplay=1&wmode=opaque&vq=hd720');
			$('body').css({'overflow' : 'none','overflow-x' : 'hidden', 'overflow' : 'hidden'});
			$(videoOverlayId).show();
			//$(videoOverlayId+', '+YoutubeVideoIframe).width($(window).width()).height($(window).height()).css({'z-index':1050});//.show();
			//$(videoOverlayId).css({'top': offset + 'px'}).fadeIn(500);
			setTimeout(function() { $(YoutubeVideoIframe).removeClass('closed'); $(videoOverlayClose).fadeIn(500); }, 500);
		}
		$(document).on('click',videoOverlayClose, function() {
			$('body').css({ overflow : 'auto','overflow-x' : 'hidden'});
			$(YoutubeVideoIframe).addClass('closed');
			setTimeout(function() {
				$(videoOverlayId).hide();
				$(YoutubeVideoIframe).attr('src','');
			}, 500);
		});

		$(document).on('click', YoutubeVideoPlay, function(){
			showVideo(YoutubeVideoID);
		});

		$(YoutubeVideoPlay).css({ top: function() {
				var onePercent = $(YoutubeVideoContainer).height() / 100;
				return ((onePercent * {$params['playButtonVerticalPosition']}) - $(YoutubeVideoPlay).find('img').height() / 2)+'px';
			}
		});
		if ($(YoutubeVideoContainer).find('.text-center').length)
		{
			$(YoutubeVideoPlay).css({ left: function() {
				return 	((0 - $(YoutubeVideoPlay).width() / 2) + $(YoutubeVideoContainer).width()/2)+'px';
			}});
		}
		$(YoutubeVideoPlay).css({ opacity: 1});

		{if isset($params['playOnLoad']) && $params['playOnLoad'] == 'on'}
			try
			{
				var cookies=getCookie(videoOverlayId);
				if(cookies == 'null' || cookies == null || cookies == "") {
					setCookie(videoOverlayId, 1, 30);
					showVideo(YoutubeVideoID);
				}
			}
			catch (e) {
				console.info('getCookie() is not availebale');
			}
		{else}
			try
			{
				setCookie(videoOverlayId, null);
			}
			catch (e) {
				console.info('getCookie() is not availebale');
			}
		{/if}
	});
</script>

<div id="Youtube-video-overlay-{$params['youtubeHash']}">
	<iframe id="Youtube-video-iframe-{$params['youtubeHash']}" src="" frameborder="0" class="closed"></iframe>
	<img id="Youtube-video-overlay-close-{$params['youtubeHash']}" src="data:image/png;base64," alt="">
</div>
<div class="container{if empty($image)} no-bg-image{else} with-bg-image{/if}" id="Youtube-video-container-{$params['youtubeHash']}">
	{*<div class="row">*}
		{*<div class="col-lg-12 bg-image">*}
		{*</div>*}
	{*</div>*}
	<div class="row">
		<img n:if="$image" src="{$basePath}/attachments/{$image->md5}" alt="{$params['imageAlt']}" class="video-bg-image"/>
		<div class="{if $params['playButtonPosition'] != 'center'}col-lg-1{else}col-lg-12{/if}
			col-lg-offset-{if $params['playButtonPosition'] > 1 && $params['playButtonPosition'] <= 12}{($params['playButtonPosition']-1)}{else}0{/if}
			{if $params['playButtonPosition'] == 'center'} text-center{/if}">
			<div id="Youtube-video-play-{$params['youtubeHash']}" style="top: {$params['playButtonVerticalPosition']|noescape}%">
				<img class="layer0" src="data:image/png;base64,">
				<img class="layer1" src="data:image/png;base64,">
			</div>
		</div>
	</div>
</div>
